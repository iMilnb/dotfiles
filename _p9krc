shellcolors() { for code ({000..255}) print -P -- "$code: %F{$code}This is how your text would look like%f" }

# prompt_user and prompt_host command not found if not.
source $ZSH/themes/powerlevel9k/powerlevel9k.zsh-theme

P9KFG='015'
POWERLEVEL9K_USER_DEFAULT_FOREGROUND=$P9KFG
POWERLEVEL9K_USER_DEFAULT_BACKGROUND='008'
POWERLEVEL9K_HOST_LOCAL_FOREGROUND=$P9KFG
POWERLEVEL9K_HOST_LOCAL_BACKGROUND='039'
POWERLEVEL9K_HOST_REMOTE_FOREGROUND=$P9KFG
POWERLEVEL9K_HOST_REMOTE_BACKGROUND='172'
POWERLEVEL9K_DIR_DEFAULT_FOREGROUND=$P9KFG
POWERLEVEL9K_DIR_DEFAULT_BACKGROUND='125'
POWERLEVEL9K_DIR_HOME_FOREGROUND=$P9KFG
POWERLEVEL9K_DIR_HOME_BACKGROUND='027'
POWERLEVEL9K_DIR_HOME_SUBFOLDER_FOREGROUND=$POWERLEVEL9K_DIR_HOME_FOREGROUND
POWERLEVEL9K_DIR_HOME_SUBFOLDER_BACKGROUND=$POWERLEVEL9K_DIR_HOME_BACKGROUND
POWERLEVEL9K_PROMPT_ON_NEWLINE=true
POWERLEVEL9K_RPROMPT_ON_NEWLINE=true
POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(user host dir)
POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(status background_jobs virtualenv vcs)

